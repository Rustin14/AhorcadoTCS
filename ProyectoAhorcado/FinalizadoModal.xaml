<Window x:Class="ProyectoAhorcado.FinalizadoModal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProyectoAhorcado"
        mc:Ignorable="d"
        Title="FinalizadoModal" Height="450" Width="600">
    <Grid>
        <Rectangle HorizontalAlignment="Center" Height="434" Stroke="Black" VerticalAlignment="Center" Width="600" Fill="#35000000"/>
        <Label x:Name="finalizadoLabel" Content="FINALIZADO" HorizontalAlignment="Left" Margin="110,0,0,0" VerticalAlignment="Center" FontFamily="Roboto" FontWeight="Bold" FontStyle="Italic" FontSize="72"/>
        <Button x:Name="botonCerrar" Content="Aceptar" HorizontalAlignment="Center" Height="40" Margin="0,303,0,0" VerticalAlignment="Top" Width="273" FontSize="18" FontFamily="Roboto" FontStyle="Italic" FontWeight="Bold" Click="botonCerrar_Click"/>

    </Grid>
</Window>
